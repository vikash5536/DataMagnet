        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataMagnetApp"
        mc:Ignorable="d"
        Title="ActivateWindow" ResizeMode="NoResize" Topmost="True" Height="300" Width="300">
    <Grid>
        <Label Height="30" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Product Key" Margin="0,72,0,170"></Label>
        <TextBox Name="txtProductKey" Height="30" HorizontalContentAlignment="Left" Padding="3,0,3,0" VerticalContentAlignment="Center" Margin="20,0,20,0"></TextBox>
    </Grid>
</Window>
